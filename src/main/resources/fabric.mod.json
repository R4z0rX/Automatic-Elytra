{
  "schemaVersion": 1,
  "id": "automaticelytra",
  "version": "${version}",

  "name": "Automatic Elytra",
  "description": "Automatic Elytra is a client side mod that will swap the players elytra and chestplate for them!",
  "authors": [
    "AreUThreateningMe",
    "RazrCraft"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/automatic-elytra",
    "sources": "https://github.com/kregerl/Automatic-Elytra"
  },

  "license": "CC0-1.0",
  "icon": "assets/automaticelytra/AutomaticElytraLogo.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.loucaskreger.automaticelytra.AutomaticElytra"
    ]
  },
  "mixins": [
    "automaticelytra.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.16.10",
    "fabric-api": ">=0.115.0+1.21.4",
    "minecraft": "~1.21.4",
    "java": ">=21"
    }
}
